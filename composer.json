{
    "name": "ucscode/uss-form",
    "description": "A PHP library designed for effortless HTML form creation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Uchenna Ajah",
            "email": "uche23mail@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ucscode/uss-element": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ucscode\\UssForm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ucscode\\UssForm\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
